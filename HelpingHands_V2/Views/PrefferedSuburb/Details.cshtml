@model HelpingHands_V2.Models.PrefferedSuburb

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>PrefferedSuburb</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nurse)
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.PrefferedSuburb.NurseFirstname @ViewBag.PrefferedSuburb.NurseLastname
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Suburb)
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.PrefferedSuburb.SuburbName
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new {  id = @ViewBag.PrefferedSuburb.NurseId  }) |
    <a asp-action="IndexForNurse" asp-route-id="@User.FindFirst("UserId")!.Value">Back to List</a>
</div>
