@model HelpingHands_V2.Models.Suburb

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Suburb</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.SuburbName)
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.Suburb.SuburbName
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PostalCode)
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.Suburb.PostalCode
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.City)
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.Suburb.CityName
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@ViewBag.Suburb.SuburbId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
