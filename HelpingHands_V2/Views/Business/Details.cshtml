@model HelpingHands_V2.Models.BusinessInformation

@{
    ViewData["Title"] = "Details";
    var Bi = ViewBag.BusinessInfo;
}

<h1>Details</h1>

<div>
    <h4>BusinessInformation</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.OrganizationName)
        </dt>
        <dd class = "col-sm-10">
            @Bi.OrganizationName
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nponumber)
        </dt>
        <dd class = "col-sm-10">
