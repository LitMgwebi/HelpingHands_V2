            @Bi.AddressLineOne
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.AddressLineTwo)
        </dt>
        <dd class = "col-sm-10">
            @Bi.AddressLineTwo
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ContactNumber)
        </dt>
        <dd class = "col-sm-10">
            @Bi.ContactNumber
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class = "col-sm-10">
            @Bi.Email
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Suburb)
        </dt>
        <dd class = "col-sm-10">
            @Bi.SuburbName
